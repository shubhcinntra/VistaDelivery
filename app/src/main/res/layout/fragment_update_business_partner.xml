<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/quatoes_main_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:clickable="true">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/headerLayout"
            layout="@layout/header_bottomround_edit"
            android:visibility="visible" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/size_15"
                android:layout_marginRight="@dimen/size_15"
                android:orientation="vertical"
                android:paddingTop="@dimen/size_15">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/lead"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_15"
                            android:fontFamily="@font/muli_regular"
                            android:text="@string/lead"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_14" />

                        <EditText
                            android:id="@+id/lead_value"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_marginTop="@dimen/size_10"
                            android:background="@drawable/rounded_border"
                            android:clickable="false"
                            android:drawableTint="@color/black"
                            android:focusable="false"
                            android:gravity="center_vertical"
                            android:hint="@string/lead"
                            android:imeOptions="actionNext"
                            android:maxLines="1"
                            android:editable="false"
                            android:paddingLeft="@dimen/size_15"
                            android:paddingRight="@dimen/size_4"
                            android:textColor="@color/black"
                            android:textColorHint="@color/light_grey_box"
                            android:textSize="@dimen/text_size_14" />


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_10"
                            android:fontFamily="@font/muli_regular"
                            android:text="@string/company_name_mand"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_14" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_10"
                            android:background="@drawable/rounded_border_edit"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/image1"
                                android:layout_width="@dimen/size_30"
                                android:layout_height="@dimen/size_30"
                                android:layout_centerVertical="true"
                                android:layout_gravity="center_vertical|center_horizontal"
                                android:layout_marginStart="@dimen/size_5"
                                android:layout_marginTop="@dimen/size_5"
                                android:layout_marginEnd="@dimen/size_5"
                                android:layout_marginBottom="@dimen/size_5"
                                android:padding="@dimen/size_5"
                                android:src="@drawable/building" />

                            <EditText
                                android:id="@+id/edCompanyName"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/text_45"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@id/image1"
                                android:background="@null"
                                android:gravity="center_vertical"
                                android:hint="@string/name"
                                android:imeOptions="actionNext"
                                android:inputType="text"
                                android:maxLength="100"
                                android:maxLines="1"
                                android:paddingLeft="@dimen/size_5"
                                android:textColorHint="@color/light_grey_box"
                                android:textSize="@dimen/text_size_14" />

                        </RelativeLayout>

                    </LinearLayout>


                    <TextView
                        android:id="@+id/mobile"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/mobile_no_mand"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/image4"
                            android:layout_width="@dimen/size_30"
                            android:layout_height="@dimen/size_30"
                            android:layout_centerVertical="true"
                            android:layout_margin="@dimen/size_5"
                            android:padding="@dimen/size_5"
                            android:src="@drawable/ic_phone_icon" />

                        <EditText
                            android:id="@+id/mobile_value"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/image4"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint="@string/mobile"
                            android:imeOptions="actionNext"
                            android:inputType="phone|number"
                            android:maxLength="10"
                            android:maxLines="1"
                            android:paddingLeft="@dimen/size_5"
                            android:paddingRight="@dimen/size_5"
                            android:textColorHint="@color/light_grey_box"
                            android:textSize="@dimen/text_size_14" />
                    </RelativeLayout>



                    <TextView
                        android:id="@+id/email"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/email_mand"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:gravity="center_horizontal|center_vertical">

                        <ImageView
                            android:id="@+id/image7"
                            android:layout_width="@dimen/size_30"
                            android:layout_height="@dimen/size_30"
                            android:layout_centerVertical="true"
                            android:layout_margin="@dimen/size_5"
                            android:padding="@dimen/size_5"
                            android:src="@drawable/ic_message" />

                        <EditText
                            android:id="@+id/email_value"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/image7"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint="@string/email"
                            android:imeOptions="actionNext"
                            android:inputType="textEmailAddress"
                            android:maxLength="100"
                            android:maxLines="1"
                            android:paddingLeft="@dimen/size_5"
                            android:paddingRight="@dimen/size_5"
                            android:textColorHint="@color/light_grey_box"
                            android:textSize="@dimen/text_size_14" />

                    </RelativeLayout>


                    <TextView
                        android:id="@+id/website"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/website_"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:gravity="center_horizontal|center_vertical">

                        <ImageView
                            android:id="@+id/image8"
                            android:layout_width="@dimen/size_30"
                            android:layout_height="@dimen/size_30"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical|center_horizontal"
                            android:layout_margin="@dimen/size_5"
                            android:padding="@dimen/size_5"
                            android:src="@drawable/ic_globe" />

                        <EditText

                            android:id="@+id/website_value"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/image8"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint="@string/website"
                            android:imeOptions="actionNext"
                            android:inputType="textUri"
                            android:maxLength="100"
                            android:maxLines="1"
                            android:paddingLeft="@dimen/size_5"
                            android:paddingRight="@dimen/size_5"
                            android:textColorHint="@color/light_grey_box"
                            android:textSize="@dimen/text_size_14" />
                    </RelativeLayout>


                    <TextView
                        android:id="@+id/headingTurnOver"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/turnover"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />

                    <EditText
                        android:id="@+id/etTurnover"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:gravity="center_vertical"
                        android:hint="@string/turnover"
                        android:imeOptions="actionNext"
                        android:inputType="number"
                        android:maxLength="120"
                        android:maxLines="1"
                        android:paddingLeft="@dimen/size_15"
                        android:textColorHint="@color/light_grey_box"
                        android:textSize="@dimen/text_size_14" />




                    <TextView
                        android:id="@+id/total"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/muli_bold"
                        android:text="@string/contact_detail"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_16" />

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/conatct_owner"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/contact_owner_mand"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />

                    <RelativeLayout
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_10"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_10">

                            <com.google.android.material.textfield.TextInputLayout
                                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Select Contact Person"
                                app:boxCornerRadiusBottomEnd="10dp"
                                app:boxCornerRadiusBottomStart="10dp"
                                app:boxCornerRadiusTopEnd="10dp"
                                app:boxCornerRadiusTopStart="10dp">

                                <AutoCompleteTextView
                                    android:id="@+id/acContactPersonName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:editable="false"
                                    android:maxLines="1"
                                    android:textSize="12sp" />

                            </com.google.android.material.textfield.TextInputLayout>

                        </RelativeLayout>

                        <!--<EditText
                            android:id="@+id/contact_owner_value"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/image2"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint="Customer Name"
                            android:imeOptions="actionNext"
                            android:inputType="text"
                            android:maxLength="100"
                            android:maxLines="1"
                            android:paddingLeft="@dimen/size_5"
                            android:textColorHint="@color/light_grey_box"
                            android:textSize="@dimen/text_size_14" />-->

                    </RelativeLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/mobile_mand"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/image5"
                            android:layout_width="@dimen/size_30"
                            android:layout_height="@dimen/size_30"
                            android:layout_centerVertical="true"
                            android:layout_margin="@dimen/size_5"
                            android:padding="@dimen/size_5"
                            android:src="@drawable/ic_phone_icon" />

                        <EditText
                            android:id="@+id/edContactPhoneNo"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/image5"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint="@string/mobile"
                            android:imeOptions="actionNext"
                            android:inputType="phone|number"
                            android:maxLength="10"
                            android:maxLines="1"
                            android:paddingLeft="@dimen/size_5"
                            android:paddingRight="@dimen/size_5"
                            android:textColorHint="@color/light_grey_box"
                            android:textSize="@dimen/text_size_14" />
                    </RelativeLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/email"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:gravity="center_horizontal|center_vertical">

                        <ImageView
                            android:id="@+id/image6"
                            android:layout_width="@dimen/size_30"
                            android:layout_height="@dimen/size_30"
                            android:layout_centerVertical="true"
                            android:layout_margin="@dimen/size_5"
                            android:padding="@dimen/size_5"
                            android:src="@drawable/ic_message" />

                        <EditText
                            android:id="@+id/edContactEmail"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/image6"
                            android:background="@null"
                            android:gravity="center_vertical"
                            android:hint="@string/email"
                            android:imeOptions="actionNext"
                            android:inputType="textEmailAddress"
                            android:maxLength="100"
                            android:maxLines="1"
                            android:paddingLeft="@dimen/size_5"
                            android:paddingRight="@dimen/size_5"
                            android:textColorHint="@color/light_grey_box"
                            android:textSize="@dimen/text_size_14" />
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_15"
                            android:fontFamily="@font/muli_regular"
                            android:text="@string/industry_mand"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_14" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_10"
                            android:gravity="center_vertical">

                            <com.google.android.material.textfield.TextInputLayout
                                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Select Industry"
                                app:boxCornerRadiusBottomEnd="10dp"
                                app:boxCornerRadiusBottomStart="10dp"
                                app:boxCornerRadiusTopEnd="10dp"
                                app:boxCornerRadiusTopStart="10dp">

                                <AutoCompleteTextView
                                    android:id="@+id/acLeadType"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:editable="false"
                                    android:maxLines="1"
                                    android:textSize="12sp" />

                            </com.google.android.material.textfield.TextInputLayout>

                        </RelativeLayout>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_15"
                            android:fontFamily="@font/muli_regular"
                            android:text="@string/sales_employee_mand"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_14" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_10">

                            <com.google.android.material.textfield.TextInputLayout
                                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Select Sales Employee"
                                app:boxCornerRadiusBottomEnd="10dp"
                                app:boxCornerRadiusBottomStart="10dp"
                                app:boxCornerRadiusTopEnd="10dp"
                                app:boxCornerRadiusTopStart="10dp">

                                <AutoCompleteTextView
                                    android:id="@+id/ac_sales_employee"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:editable="false"
                                    android:maxLines="1"
                                    android:textSize="12sp" />

                            </com.google.android.material.textfield.TextInputLayout>

                        </RelativeLayout>

                    </LinearLayout>

                    <TextView
                        android:id="@+id/headingBusinessType"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/business_type"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />

                    <RelativeLayout
                        android:id="@+id/businessType_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:gravity="center_vertical"
                        android:paddingRight="@dimen/size_10">

                        <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                            android:id="@+id/saerchableSpinnerBusinessType"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"

                            android:clickable="true"
                            android:dropDownWidth="wrap_content"
                            android:focusable="false"
                            android:forceHasOverlappingRendering="false"
                            android:gravity="center_vertical|start"
                            android:paddingLeft="@dimen/size_5"
                            android:spinnerMode="dropdown"
                            android:textSize="@dimen/text_size_14" />


                    </RelativeLayout>

                    <TextView
                        android:id="@+id/payment_term"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/payment_term_mand"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:gravity="center_horizontal|center_vertical"
                        android:paddingRight="@dimen/size_10">


                        <Spinner
                            android:id="@+id/payment_term_value"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_toLeftOf="@id/img3"
                            android:background="@null"
                            android:dropDownWidth="wrap_content"
                            android:dropDownVerticalOffset="@dimen/text_45"
                            android:forceHasOverlappingRendering="false"
                            android:gravity="center_vertical|start"
                            android:paddingLeft="@dimen/size_5"
                            android:spinnerMode="dropdown"
                            android:textSize="@dimen/text_size_14" />

                        <ImageView
                            android:id="@+id/img3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:src="@drawable/ic_bottom_arrow_black" />
                    </RelativeLayout>


                    <TextView
                        android:id="@+id/headerZone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/zone_mand"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />

                    <RelativeLayout
                        android:id="@+id/zone_relative_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:gravity="center_vertical"
                        android:paddingRight="@dimen/size_10">

                        <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                            android:id="@+id/saerchableSpinnerZone"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"

                            android:clickable="true"
                            android:dropDownWidth="wrap_content"
                            android:focusable="false"
                            android:forceHasOverlappingRendering="false"
                            android:gravity="center_vertical|start"
                            android:paddingLeft="@dimen/size_5"
                            android:spinnerMode="dropdown"
                            android:textSize="@dimen/text_size_14" />


                    </RelativeLayout>





                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:visibility="visible">


                        <TextView
                            android:id="@+id/invoice_no"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_15"
                            android:fontFamily="@font/muli_regular"
                            android:text="@string/gst"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_14" />

                        <EditText
                            android:id="@+id/invoice_no_value"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_marginTop="@dimen/size_10"
                            android:background="@drawable/rounded_border"
                            android:gravity="center_vertical"
                            android:hint="@string/gst"
                            android:imeOptions="actionNext"
                            android:maxLength="12"
                            android:maxLines="1"
                            android:paddingLeft="@dimen/size_15"
                            android:textColorHint="@color/light_grey_box"
                            android:textSize="@dimen/text_size_14" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:visibility="visible">


                        <TextView
                            android:id="@+id/parent_account"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_15"
                            android:fontFamily="@font/muli_regular"
                            android:text="@string/parent_account"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_14" />


                        <RelativeLayout
                            android:id="@+id/parent_account_relative_view"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/size_10"
                            android:background="@drawable/rounded_border"
                            android:gravity="center_vertical"
                            android:paddingRight="@dimen/size_10">

                            <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                                android:id="@+id/saerchableSpinnerParentAccount"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/text_45"

                                android:clickable="true"
                                android:dropDownWidth="wrap_content"
                                android:focusable="false"
                                android:forceHasOverlappingRendering="false"
                                android:gravity="center_vertical|start"
                                android:paddingLeft="@dimen/size_5"
                                android:spinnerMode="dropdown"
                                android:textSize="@dimen/text_size_14" />


                        </RelativeLayout>

                    </LinearLayout>


                </LinearLayout>


                <include
                    android:id="@+id/addressSectionLayout"
                    layout="@layout/updateaddress_section" />


                <TextView
                    android:id="@+id/remarks"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="Notes"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <EditText
                    android:id="@+id/remarks_value"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/size_100"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:gravity="start"
                    android:hint="Notes"
                    android:imeOptions="actionDone"
                    android:inputType="textMultiLine"
                    android:maxLength="200"
                    android:maxLines="5"
                    android:padding="@dimen/size_15"
                    android:textColorHint="@color/light_grey_box"
                    android:textSize="@dimen/text_size_14" />


                <Button
                    android:id="@+id/updateButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_50"
                    android:layout_marginBottom="@dimen/size_15"
                    android:background="@drawable/button_rounded_corner"
                    android:clickable="true"
                    android:focusable="true"
                    android:fontFamily="@font/muli_regular"
                    android:gravity="center"
                    android:text="Update"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_18"
                    app:backgroundTint="@color/colorPrimary" />

            </LinearLayout>

        </ScrollView>

    </LinearLayout>

    <include
        android:id="@+id/loaderLayout"
        layout="@layout/loader"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


</FrameLayout>
