<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/quatoes_main_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".fragments.BlankFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/header"
            android:scrollbars="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/size_15"
                android:layout_marginRight="@dimen/size_15"
                android:orientation="vertical"
                android:paddingTop="@dimen/size_15">


                <TextView
                    android:id="@+id/lead"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/lead"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <EditText
                    android:id="@+id/lead_value"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/text_45"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:clickable="false"
                    android:drawableRight="@drawable/ic_baseline_keyboard_arrow_right_24"
                    android:drawableTint="@color/black"
                    android:focusable="false"
                    android:gravity="center_vertical"
                    android:hint="@string/lead"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/size_15"
                    android:paddingRight="@dimen/size_4"
                    android:textColor="@color/black"
                    android:textColorHint="@color/light_grey_box"
                    android:textSize="@dimen/text_size_14" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border_edit"
                    android:paddingRight="@dimen/size_10"
                    android:visibility="gone">

                    <Spinner
                        android:id="@+id/lead_spinner"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:layout_toLeftOf="@id/drov_arrow22"
                        android:background="@null"
                        android:dropDownWidth="wrap_content"
                        android:dropDownVerticalOffset="@dimen/text_45"
                        android:forceHasOverlappingRendering="false"
                        android:gravity="center_vertical|start"
                        android:paddingLeft="@dimen/size_5"
                        android:spinnerMode="dropdown"
                        android:textSize="@dimen/text_size_14" />

                    <ImageView
                        android:id="@+id/drov_arrow22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/ic_bottom_arrow_black" />

                </RelativeLayout>

                <TextView
                    android:id="@+id/first_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/company_name_mand"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border_edit"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/image1"
                        android:layout_width="@dimen/size_30"
                        android:layout_height="@dimen/size_30"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical|center_horizontal"
                        android:layout_margin="@dimen/size_5"
                        android:padding="@dimen/size_5"
                        android:src="@drawable/building" />

                    <EditText
                        android:id="@+id/name_value"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@id/image1"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:hint="@string/name"
                        android:imeOptions="actionNext"
                        android:inputType="text"
                        android:maxLength="100"
                        android:maxLines="1"
                        android:paddingLeft="@dimen/size_5"
                        android:textColorHint="@color/light_grey_box"
                        android:textSize="@dimen/text_size_14" />

                </RelativeLayout>


                <TextView
                    android:id="@+id/company_no"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/mobile_no_mand"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border_edit"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/comp_phon"
                        android:layout_width="@dimen/size_30"
                        android:layout_height="@dimen/size_30"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical|center_horizontal"
                        android:layout_margin="@dimen/size_5"
                        android:padding="@dimen/size_5"
                        android:src="@drawable/ic_phone_icon" />

                    <EditText
                        android:id="@+id/company_no_value"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@id/comp_phon"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:hint="Mobile No"
                        android:imeOptions="actionNext"
                        android:inputType="number"
                        android:maxLength="10"
                        android:maxLines="1"
                        android:paddingLeft="@dimen/size_5"
                        android:textColorHint="@color/light_grey_box"
                        android:textSize="@dimen/text_size_14" />

                </RelativeLayout>


                <TextView
                    android:id="@+id/website"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/website_"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:gravity="center_horizontal|center_vertical">

                    <ImageView
                        android:id="@+id/image6"
                        android:layout_width="@dimen/size_30"
                        android:layout_height="@dimen/size_30"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical|center_horizontal"
                        android:layout_margin="@dimen/size_5"
                        android:padding="@dimen/size_5"
                        android:src="@drawable/ic_globe" />

                    <EditText

                        android:id="@+id/website_value"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@id/image6"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:hint="@string/website"
                        android:imeOptions="actionNext"
                        android:inputType="textUri"
                        android:maxLength="100"
                        android:maxLines="1"
                        android:paddingLeft="@dimen/size_5"
                        android:paddingRight="@dimen/size_5"
                        android:textColorHint="@color/light_grey_box"
                        android:textSize="@dimen/text_size_14" />
                </RelativeLayout>


                <TextView
                    android:id="@+id/company_email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/email_mand"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border_edit"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/comp_email"
                        android:layout_width="@dimen/size_30"
                        android:layout_height="@dimen/size_30"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical|center_horizontal"
                        android:layout_margin="@dimen/size_5"
                        android:padding="@dimen/size_5"
                        android:src="@drawable/ic_email_new" />

                    <EditText
                        android:id="@+id/company_email_value"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@id/comp_email"
                        android:background="@null"
                        android:gravity="center_vertical"
                        android:hint="@string/email"
                        android:imeOptions="actionNext"
                        android:inputType="textEmailAddress"
                        android:maxLength="100"
                        android:maxLines="1"
                        android:paddingLeft="@dimen/size_5"
                        android:textColorHint="@color/light_grey_box"
                        android:textSize="@dimen/text_size_14" />

                </RelativeLayout>


                <TextView
                    android:id="@+id/headingTurnOver"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/turnover"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <EditText
                    android:id="@+id/etTurnover"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/text_45"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:gravity="center_vertical"
                    android:hint="@string/turnover"
                    android:imeOptions="actionNext"
                    android:inputType="number"
                    android:maxLength="120"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/size_15"
                    android:textColorHint="@color/light_grey_box"
                    android:textSize="@dimen/text_size_14" />


                <TextView
                    android:id="@+id/industry"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/industry"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <RelativeLayout
                    android:id="@+id/industry_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:gravity="center_vertical"
                    android:paddingRight="@dimen/size_10">

                    <Spinner
                        android:id="@+id/industry_spinner"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:background="@null"
                        android:clickable="true"
                        android:dropDownWidth="wrap_content"
                        android:focusable="false"
                        android:forceHasOverlappingRendering="false"
                        android:gravity="center_vertical|start"
                        android:paddingLeft="@dimen/size_5"
                        android:spinnerMode="dropdown"
                        android:textSize="@dimen/text_size_14" />

                    <ImageView

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/ic_bottom_arrow_black"
                        android:clickable="true"
                        android:focusable="false" />

                </RelativeLayout>


                <TextView
                    android:id="@+id/card"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/company_id"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14"
                    android:visibility="gone" />


                <EditText
                    android:id="@+id/cardCode_value"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/text_45"
                    android:layout_centerVertical="true"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:gravity="center_vertical"
                    android:hint="@string/cardcode"
                    android:imeOptions="actionNext"
                    android:inputType="text"
                    android:maxLength="100"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/size_15"
                    android:textColorHint="@color/light_grey_box"
                    android:textSize="@dimen/text_size_14"
                    android:visibility="gone"

                    />


                <TextView
                    android:id="@+id/sales_employee"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/sales_employee_mand"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10">

                    <com.google.android.material.textfield.TextInputLayout
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Select Sales Employee"
                        app:boxCornerRadiusBottomEnd="10dp"
                        app:boxCornerRadiusBottomStart="10dp"
                        app:boxCornerRadiusTopEnd="10dp"
                        app:boxCornerRadiusTopStart="10dp">

                        <AutoCompleteTextView
                            android:id="@+id/acSalesEmployee"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:editable="false"
                            android:maxLines="1"
                            android:textSize="12sp" />

                    </com.google.android.material.textfield.TextInputLayout>


                    <!-- <Spinner
                         android:id="@+id/sales_employee_spinner"
                         android:layout_width="match_parent"
                         android:layout_height="@dimen/text_45"
                         android:layout_toLeftOf="@id/drov_arrow"
                         android:background="@null"
                         android:dropDownWidth="wrap_content"
                         android:dropDownVerticalOffset="@dimen/text_45"
                         android:forceHasOverlappingRendering="false"
                         android:gravity="center_vertical|start"
                         android:paddingLeft="@dimen/size_5"
                         android:spinnerMode="dropdown"
                         android:textSize="@dimen/text_size_14" />

                     <ImageView
                         android:id="@+id/drov_arrow"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:layout_alignParentRight="true"
                         android:layout_centerVertical="true"
                         android:src="@drawable/ic_bottom_arrow_black" />-->

                </RelativeLayout>


                <TextView
                    android:id="@+id/headingBusinessType"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/business_type"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <RelativeLayout
                    android:id="@+id/businessType_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:gravity="center_vertical"
                    android:paddingRight="@dimen/size_10">

                    <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                        android:id="@+id/saerchableSpinnerBusinessType"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"

                        android:clickable="true"
                        android:dropDownWidth="wrap_content"
                        android:focusable="false"
                        android:forceHasOverlappingRendering="false"
                        android:gravity="center_vertical|start"
                        android:paddingLeft="@dimen/size_5"
                        android:spinnerMode="dropdown"
                        android:textSize="@dimen/text_size_14" />


                </RelativeLayout>


                <TextView
                    android:id="@+id/company_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/company_id"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14"
                    android:visibility="gone" />

                <EditText
                    android:id="@+id/company_id_value"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/text_45"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:gravity="center_vertical"
                    android:hint="@string/company_id"
                    android:imeOptions="actionNext"
                    android:inputType="number"
                    android:maxLength="12"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/size_15"
                    android:textColorHint="@color/light_grey_box"
                    android:textSize="@dimen/text_size_14"
                    android:visibility="gone" />





                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="visible">


                    <TextView
                        android:id="@+id/type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/type"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14"
                        android:visibility="gone" />


                    <RelativeLayout
                        android:id="@+id/type_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:gravity="center_vertical"
                        android:paddingRight="@dimen/size_10"
                        android:visibility="gone">

                        <Spinner
                            android:id="@+id/type_spinner"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_gravity="center_horizontal|center_vertical"
                            android:background="@null"
                            android:clickable="true"
                            android:dropDownWidth="wrap_content"
                            android:entries="@array/opp_type"
                            android:focusable="false"
                            android:forceHasOverlappingRendering="false"
                            android:gravity="center_horizontal|center_vertical"
                            android:paddingLeft="@dimen/size_5"
                            android:spinnerMode="dropdown"
                            android:textColorHint="@color/light_grey_box"
                            android:textSize="@dimen/text_size_14" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/ic_bottom_arrow_black"
                            android:clickable="true"
                            android:focusable="false" />

                    </RelativeLayout>





                    <TextView
                        android:id="@+id/payment_term"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/payment_term_mand"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:gravity="center_horizontal|center_vertical"
                        android:paddingRight="@dimen/size_10">


                        <Spinner
                            android:id="@+id/payment_term_value"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"
                            android:layout_toLeftOf="@id/img3"
                            android:background="@null"
                            android:dropDownWidth="wrap_content"
                            android:dropDownVerticalOffset="@dimen/text_45"
                            android:forceHasOverlappingRendering="false"
                            android:gravity="center_vertical|start"
                            android:paddingLeft="@dimen/size_5"
                            android:spinnerMode="dropdown"
                            android:textSize="@dimen/text_size_14" />

                        <ImageView
                            android:id="@+id/img3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:src="@drawable/ic_bottom_arrow_black" />
                    </RelativeLayout>


                    <TextView
                        android:id="@+id/headerZone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/zone_mand"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14" />

                    <RelativeLayout
                        android:id="@+id/zone_relative_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:gravity="center_vertical"
                        android:paddingRight="@dimen/size_10">

                        <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                            android:id="@+id/saerchableSpinnerZone"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/text_45"

                            android:clickable="true"
                            android:dropDownWidth="wrap_content"
                            android:focusable="false"
                            android:forceHasOverlappingRendering="false"
                            android:gravity="center_vertical|start"
                            android:paddingLeft="@dimen/size_5"
                            android:spinnerMode="dropdown"
                            android:textSize="@dimen/text_size_14" />


                    </RelativeLayout>

                    <TextView
                        android:id="@+id/ratings"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/size_15"
                        android:fontFamily="@font/muli_regular"
                        android:text="@string/ratings"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_14"
                        android:visibility="gone" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:layout_marginTop="@dimen/size_10"
                        android:background="@drawable/rounded_border"
                        android:visibility="gone">

                        <RatingBar
                            android:id="@+id/ratingBar"
                            style="@style/Widget.AppCompat.RatingBar.Indicator"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:clickable="true"
                            android:focusable="true"
                            android:isIndicator="false"
                            android:numStars="5"
                            android:progressTint="@color/safron_barChart"
                            android:scaleX="0.8"
                            android:scaleY="0.8" />
                    </RelativeLayout>

                </LinearLayout>


                <TextView
                    android:id="@+id/parent_account"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/parent_account"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <RelativeLayout
                    android:id="@+id/parent_account_relative_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:gravity="center_vertical"
                    android:paddingRight="@dimen/size_10">

                    <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                        android:id="@+id/saerchableSpinnerParentAccount"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"

                        android:clickable="true"
                        android:dropDownWidth="wrap_content"
                        android:focusable="false"
                        android:forceHasOverlappingRendering="false"
                        android:gravity="center_vertical|start"
                        android:paddingLeft="@dimen/size_5"
                        android:spinnerMode="dropdown"
                        android:textSize="@dimen/text_size_14" />


                </RelativeLayout>

                <TextView
                    android:id="@+id/invoice_no"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/gst"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <EditText
                    android:id="@+id/invoice_no_value"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/text_45"
                    android:layout_marginTop="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:gravity="center_vertical"
                    android:hint="@string/gst"
                    android:imeOptions="actionNext"
                    android:maxLength="15"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/size_15"
                    android:textColorHint="@color/light_grey_box"
                    android:textSize="@dimen/text_size_14" />

                <TextView
                    android:id="@+id/headingRemarks"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_15"
                    android:fontFamily="@font/muli_regular"
                    android:text="@string/remarks"
                    android:visibility="gone"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14" />

                <EditText
                    android:id="@+id/etRemarksgeneral"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/text_45"
                    android:layout_marginTop="@dimen/size_10"
                    android:layout_marginBottom="@dimen/size_10"
                    android:background="@drawable/rounded_border"
                    android:gravity="center_vertical"
                    android:hint="@string/remarks"
                    android:imeOptions="actionNext"
                    android:inputType="numberDecimal"
                    android:maxLength="15"
                    android:maxLines="1"
                    android:visibility="gone"
                    android:paddingLeft="@dimen/size_15"
                    android:textColorHint="@color/light_grey_box"
                    android:textSize="@dimen/text_size_14" />


                <RelativeLayout
                    android:id="@+id/prntacc_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_10"
                    android:layout_marginBottom="30dp"
                    android:background="@drawable/rounded_border"
                    android:gravity="center_vertical"
                    android:paddingRight="@dimen/size_10"
                    android:visibility="gone">

                    <Spinner
                        android:id="@+id/parent_account_value"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/text_45"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:background="@null"
                        android:clickable="true"
                        android:dropDownWidth="wrap_content"
                        android:focusable="false"
                        android:forceHasOverlappingRendering="false"
                        android:gravity="center_horizontal|center_vertical"
                        android:paddingLeft="@dimen/size_5"
                        android:spinnerMode="dropdown"
                        android:textColorHint="@color/light_grey_box"
                        android:textSize="@dimen/text_size_14" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/ic_bottom_arrow_black"
                        android:clickable="true"
                        android:focusable="false" />

                </RelativeLayout>




            </LinearLayout>

        </ScrollView>

    </LinearLayout>
</FrameLayout>
